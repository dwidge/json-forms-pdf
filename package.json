{
  "private": "true",
  "name": "@dwidge/json-forms-pdf",
  "version": "0.0.6",
  "description": "React Pdf renderers for Json Forms.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwidge/json-forms-pdf.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "rimraf dist && ncp assets/ dist/ && tsc -b . tsconfig.cjs.json",
    "dev": "tsc -w",
    "test": "node --test"
  },
  "peerDependencies": {
    "@dwidge/class-name-rnw": "^0.0.6",
    "@jsonforms/core": "^3.4.0",
    "@jsonforms/react": "^3.4.0",
    "@react-pdf/layout": "3.6.4",
    "@react-pdf/textkit": "4.2.1",
    "@react-pdf/renderer": "3.1.17",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "ajv": "^8.17.1",
    "assert": "^2.1.0",
    "react": "^19.0.0",
    "react-native": "^0.79.5",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "expect": "^29.7.0",
    "ncp": "^2.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "@react-pdf/layout": "3.6.4",
      "@react-pdf/textkit": "4.2.1"
    }
  }
}
